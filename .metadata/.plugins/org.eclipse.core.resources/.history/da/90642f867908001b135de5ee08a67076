#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int x[10],n,i,num;
	printf("enter num of elements");
	fflush(stdout);
	scanf("%d",&n);
	printf("enter elements");
	for (i=0;i<=n;i++)
	{
		fflush(stdout);
		scanf("%d",&x[i]);
	}
	for (i=0;i<=n;i++)
	{

		printf("%d",x[i]);
	}
	printf("enter element to be searched");
	fflush(stdout);
	scanf("%d",&num);
	for (i=0;i<n;i++)
	while(num == x[i])
	{
		printf("%d",i);
	}
	return 0;
}
