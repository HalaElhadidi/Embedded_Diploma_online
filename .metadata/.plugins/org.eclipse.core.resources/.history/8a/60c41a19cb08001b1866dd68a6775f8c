#include <stdio.h>
#include <stdlib.h>
#define size 10
int main(void) {
	char x[size],n;
	int i,j=0;
	printf("enter a string");
	fflush(stdout);
	scanf("%s",x);
	printf("enter char");
	fflush(stdout);
	scanf("%c",&n);
	for (i=0;x[i]!='\0';i++)
	{
	if (n == x[i])
	{
		j=j+1;
	}
	printf("frequency of %c = %d",n,j);
	}
	return 0;
}
