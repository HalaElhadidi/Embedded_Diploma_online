#include <stdio.h>
#include <stdlib.h>

int main(void) {
	struct data
	{
		char Employee_Name ;
		int Employee_ID;
	};
	struct data* employee[5];
	int *ptr = employee;
	ptr->Employee_Name="Alex";

	return 0;
}
