#include <stdio.h>
#include <stdlib.h>
struct student
{
	int roll;
	char name[10];
	int mark;
};
int i;
struct student data (void)
{

	struct student info[10] ;
	printf("enter student data");
	fflush (stdout);
	for (i=0;i<10;i++)
	{   printf(" roll number");
	    fflush(stdout);
		scanf("%d",&info[i].roll);
		printf("student name");
	     fflush(stdout);
	   scanf("%s",&info[i].name);
	   printf("student marks");
	   	    fflush(stdout);
	   		scanf("%d",&info[i].mark);
	   		return info[i];
	}

}
void print (struct student info[])
{
	for(i=0;i<10;i++)
	{
		printf("student roll:%d\n student name: %s student mark \n %d",info[i].roll,info[i].name,info[i].mark);
	}
}
int main(void) {
	struct student student_data;
	for (i=0;i<10;i++)
	{
	student_date=data();
	print(student_date);
	}

	return 0;
}
